{
  "name": "@involvex/super-agent-cli",
  "version": "0.0.52",
  "description": "An open-source AI agent that brings the power of Super Agent directly into your terminal.",
  "keywords": [
    "cli",
    "agent",
    "text-editor",
    "ai"
  ],
  "homepage": "https://involvex.github.io/super-agent-cli/",
  "repository": {
    "type": "git",
    "url": "https://github.com/involvex/super-agent-cli.git"
  },
  "funding": "https://github.com/sponsors/involvex",
  "license": "MIT",
  "author": "InvolveX",
  "type": "module",
  "exports": {
    ".": {
      "types": "./dist/index.d.ts",
      "import": "./dist/index.js"
    }
  },
  "main": "dist/index.js",
  "bin": {
    "super-agent": "dist/index.js"
  },
  "scripts": {
    "prebuild": "bun run format && bun run lint:fix && bun run typecheck",
    "build": "bun build src/index.ts --outdir ./dist --target node --packages external --format esm",
    "build:bun": "bun build src/index.ts --outdir ./dist --target node --packages external --format esm",
    "changelog": "conventional-changelog -p angular -i CHANGELOG.md -s -r 0",
    "compile": "bun run prebuild && bun build --compile src/index.ts --outfile ./dist/super-agent-cli.exe --config bunfig.toml",
    "dev": "bun run --watch src/index.ts",
    "dev:node": "tsx src/index.ts",
    "format": "prettier --write .",
    "format:check": "prettier --check .",
    "install:bun": "bun install",
    "lint": "eslint . --ext .js,.jsx,.ts,.tsx",
    "lint:fix": "eslint . --ext .js,.jsx,.ts,.tsx --fix",
    "start": "node dist/index.js",
    "start:bun": "bun run dist/index.js",
    "typecheck": "tsc --noEmit"
  },
  "dependencies": {
    "@google/generative-ai": "^0.24.1",
    "@modelcontextprotocol/sdk": "^1.25.3",
    "axios": "^1.13.4",
    "cfonts": "^3.3.1",
    "chalk": "^5.6.2",
    "commander": "^14.0.2",
    "conventional-changelog-cli": "^5.0.0",
    "dotenv": "^17.2.3",
    "enquirer": "^2.4.1",
    "fs-extra": "^11.3.3",
    "ink": "^6.6.0",
    "marked": "^17.0.1",
    "marked-terminal": "^7.3.0",
    "mime": "^4.1.0",
    "openai": "^6.17.0",
    "react": "^19.2.4",
    "ripgrep-node": "^1.0.0",
    "tiktoken": "^1.0.22"
  },
  "devDependencies": {
    "@types/fs-extra": "^11.0.4",
    "@types/node": "^25.1.0",
    "@types/react": "18.3.3",
    "@typescript-eslint/eslint-plugin": "^8.54.0",
    "@typescript-eslint/parser": "^8.54.0",
    "eslint": "^9.39.2",
    "prettier": "^3.8.1",
    "prettier-plugin-organize-imports": "^4.3.0",
    "prettier-plugin-packagejson": "^3.0.0",
    "prettier-plugin-sort-imports": "^1.8.9",
    "react-devtools-core": "^7.0.1",
    "tsx": "^4.21.0",
    "typescript": "^5.9.3",
    "typescript-eslint": "^8.54.0"
  },
  "engines": {
    "node": ">=18.0.0"
  },
  "preferGlobal": true,
  "sponsor": {
    "url": "https://github.com/sponsors/involvex"
  }
}
